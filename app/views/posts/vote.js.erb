<% if @vote.valid? %>
    $("#<%=params[:vote]%>_<%=@post.id%>_vote").css("opacity", "0.5")
<% else %>
  <% vote= Vote.where(user_id:current_user, voteable_id:@vote.voteable_id,voteable_type:@vote.voteable_type) %>
  <% vote.delete(vote) %>
  $("#<%=params[:vote]%>_<%=@post.id%>_vote").css("opacity", "1")
<% end %>

$('#post_<%=@post.id%>_votes').html('<%=@post.total_votes%>')

