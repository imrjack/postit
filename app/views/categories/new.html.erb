<%= render 'shared/content_title', title: 'Create A New Category' %>

  <%=render 'shared/errors', obj:@category%>


<div class='well'>

  <%=form_for @category do |f| %>
  <div class='control-group'>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  
    <%= f.submit :submit, class: 'btn btn-success' %>
 
  <% end %>

</div>