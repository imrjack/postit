<%= render '/shared/errors', obj: @user %>
<div class='well span6'>
<%= form_for @user do |f| %>
<div class ='group-control'>
<%= f.label :username %>
<%= f.text_field :username%>
</div>

<div class='group-control'>
<%=f.label :password%>
<%=f.password_field :password%>
</div>

<%= f.submit :class => "btn btn-small" %>
<% end %>

</div>